{
  "manifest_version": 2,

  "name": "Twitch Following List CSS",
  "version": "0.0.3",
  "description": "Creates a styling for the followed channel list to make them bigger",
  "background": {
    "scripts":[ "call.js" ]
  },
  "icons": {
          "16":"images/icon-16.png",
          "48":"images/icon-48.png",
          "128":"images/icon-128.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "webNavigation",
    "*://*/*"
  ]
}
